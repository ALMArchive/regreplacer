    "@babel/register": "^7.0.0-beta.40",
    "@std/esm": "^0.20.0",
    "babel-loader": "^8.0.0-beta.0",
    "chai": "^4.1.2",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.9.0",
